@page
@model ShitoRyuSatokia.Views.Admin.IndexModel
@{
}




<div id="contact" class="contact-us section" >
    <div class="container" >
        <div class="row">
            <div class="col-lg-6 wow fadeInUp" style="margin-left: 10%;" data-wow-duration="0.5s" data-wow-delay="0.25s">
                <form id="contact" action="" method="post">
                    <div class="row">
                        <div class="col-lg-6 offset-lg-3">
                            <div class="section-heading">
                                <h6>Login</h6>
                                
                            </div>
                        </div>
                        <div class="col-lg-12">
                            <div class="row">
                                <div class="col-lg-12">
                                    <fieldset>
                                        <input type="text" name="name" id="login-email" placeholder="Email" autocomplete="on" required>
                                    </fieldset>
                                </div>
                                <div class="col-lg-12">
                                    <fieldset>
                                        <input type="password" name="surname" id="surname" placeholder="Password" autocomplete="on" required>
                                    </fieldset>
                                </div>
                               
                                <div class="col-lg-12">
                                    <fieldset>
                                        <button type="submit" id="loginForm" class="main-button ">Login</button>
                                    </fieldset>
                                </div>
                            </div>
                        </div>
                       
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>


